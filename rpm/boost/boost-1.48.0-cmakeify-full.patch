+      DEPENDS boost_graph_parallel boost_system
+      COMPILE_FLAGS "${MPI_COMPILE_FLAGS}"
+      LINK_FLAGS "${MPI_LINK_FLAGS}"
+      LINK_LIBS ${MPI_LIBRARIES}
+      SHARED_COMPILE_FLAGS "-DBOOST_MPI_DYN_LINK=1")
